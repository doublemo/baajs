{
  "name": "baajs",
  "version": "1.0.0",
  "description": "A js sdk for baa",
  "main": ".out/index.js",
  "types": ".out/index.d.ts",
  "files": [
    "dist/*",
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "bundle": "webpack",
    "dev": "webpack serve --open --config webpack.dev.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doublemo/baajs.git"
  },
  "keywords": [
    "baa",
    "baasdk",
    "webrtc"
  ],
  "author": "Randy Ma <435420057@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/doublemo/baajs/issues"
  },
  "homepage": "https://github.com/doublemo/baajs#readme",
  "devDependencies": {
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "big-integer": "^1.6.48",
    "protobufjs": "^6.11.2",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
